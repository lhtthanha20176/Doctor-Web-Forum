@model IEnumerable<Doctorsforum.Models.Professional>

@{
  ViewBag.Title = "Index";
  Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container">
  <h2>Professional</h2>

  <p>
    @Html.ActionLink("Create Professional", "Create")
  </p>
  <table class="table table-bordered bg-light">
    <thead>
      <tr>
        <th>
          @Html.DisplayNameFor(model => model.ProName)
        </th>
        <th>
          @Html.DisplayNameFor(model => model.Issued_by)
        </th>
        <th>
          @Html.DisplayNameFor(model => model.Date_Range)
        </th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      @foreach (var item in Model) {
        <tr>
          <td>
            @Html.DisplayFor(modelItem => item.ProName)
          </td>
          <td>
            @Html.DisplayFor(modelItem => item.Issued_by)
          </td>
          <td>
            @Html.DisplayFor(modelItem => item.Date_Range)
          </td>
          <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.ProID }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.ProID })
          </td>
        </tr>
      }
    </tbody>
  </table>
</div>


<hr />

<div class="text-center mt-5">
  <a href="/Home/Profileuser/@Session["id"]">Back to Profile</a>
</div>